{
    "name": "westacks/laravel-installer",
    "description": "Automaticaly istall, configure and update your client's Laravel apps from source using web interface.",
    "type": "library",
    "homepage": "https://github.com/westacks/laravel-installer",
    "license": "MIT",
    "authors": [
        {
            "name": "PunyFlash",
            "email": "puny.flash@gmail.com",
            "homepage": "https://github.com/PunyFlash"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5 || ^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "WeStacks\\Laravel\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeStacks\\Laravel\\Installer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "testdox": "phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                
            ],
            "aliases": {

            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
